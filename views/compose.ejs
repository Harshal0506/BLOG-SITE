<%- include('partial/header'); %>
<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
  <label for="Title">Title</label>
  <input id="Title" type="text" name="postTitle" class="form-control"/>
  </div>
  <div class="form-group">
  <label for="Body">Post</label>
  <textarea id="Body" name="postBody" rows="5" cols="30" class="form-control"></textarea>
  </div>
  <button class="btn btn-primary btn-lg" type="submit" name="button">Publish</button>

</form>
<%- include('partial/footer'); %>
